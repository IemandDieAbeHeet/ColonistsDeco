<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="BuildingBase">
  <defName>DECOPosterStar</defName>
  <label>decorative poster</label>
  <description>A decorative poster. Hung up by one of your colonists.</description>
  <category>Building</category>
	<thingClass>ColonistsDeco.WallDeco_Poster</thingClass>
	<graphicData>
    <texPath>Bedroom/Wall/Posters/PosterStar/Poster</texPath>
    <graphicClass>Graphic_Multi</graphicClass>
    <drawSize>(1,1)</drawSize>
  </graphicData>
  <altitudeLayer>BuildingOnTop</altitudeLayer>
  <passability>Standable</passability>
	<statBases>
    <MaxHitPoints>10</MaxHitPoints>
    <Beauty>50</Beauty>
    <WorkToBuild>100</WorkToBuild>
    <Flammability>10</Flammability>
    <Mass>2</Mass>
  </statBases>
  <comps>
    <li Class="ColonistsDeco.CompProperties_Decoration">
      <decorationName>The Original</decorationName>
    </li>
  </comps>
  <size>(1,1)</size>
  <building>
    <forceShowRoomStats>true</forceShowRoomStats>
    <isEdifice>false</isEdifice>
    <canPlaceOverWall>true</canPlaceOverWall>
  </building>
  <rotatable>true</rotatable>
	<selectable>true</selectable>
	<destroyable>true</destroyable>
	<useHitPoints>true</useHitPoints>
	<defaultPlacingRot>South</defaultPlacingRot>
  <modExtensions>
    <li Class="ColonistsDeco.DecoModExtension">
      <decoTechLevels>
        <li>Industrial</li>
      </decoTechLevels>
      <decoLocationType>Wall</decoLocationType>
    </li>
  </modExtensions>
  </ThingDef>

