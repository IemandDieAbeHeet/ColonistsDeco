<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="BuildingBase">
    <defName>DECOBoarhead</defName>
    <label>decorative boarhead</label>
    <description>A decorative boarhead. Hung up by one of your colonists.</description>
    <category>Building</category>
    <thingClass>ColonistsDeco.WallDeco_Default</thingClass>
    <graphicData>
        <texPath>Bedroom/Wall/Ornaments/Boarhead/Boarhead</texPath>
        <graphicClass>Graphic_Multi</graphicClass>
        <drawSize>(1,1)</drawSize>
    </graphicData>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <passability>Standable</passability>
    <statBases>
        <MaxHitPoints>10</MaxHitPoints>
        <Beauty>50</Beauty>
        <WorkToBuild>100</WorkToBuild>
        <Flammability>10</Flammability>
        <Mass>2</Mass>
    </statBases>
    <size>(1,1)</size>
    <building>
        <forceShowRoomStats>true</forceShowRoomStats>
        <isEdifice>false</isEdifice>
        <canPlaceOverWall>true</canPlaceOverWall>
    </building>
    <rotatable>true</rotatable>
    <selectable>true</selectable>
    <destroyable>true</destroyable>
    <useHitPoints>true</useHitPoints>
    <defaultPlacingRot>South</defaultPlacingRot>
    <modExtensions>
        <li Class="ColonistsDeco.DecoModExtension">
            <decoTechLevel>Neolithic</decoTechLevel>
            <decoLocationType>Wall</decoLocationType>
        </li>
    </modExtensions>
</ThingDef>

</Defs>
